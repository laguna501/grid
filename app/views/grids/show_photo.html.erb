<div id="photo">
  <%= image_tag @photo_source, class: "frame" %>
  <div>
    <div class="fb-like" data-href="http://localhost:3000/grids/show_photo?photo_source=<%= @photo_source %>" data-send="true" data-width="450" data-show-faces="true" data-font="verdana"></div>
    <div class="fb-comments" data-href="http://localhost:3000/grids/show_photo?photo_source=<%= @photo_source %>" data-num-posts="2" data-width="470"></div>
  </div>
</div>
